{
  "type": "module",
  "name": "@sap-ai-sdk/sample-code",
  "version": "0.0.1",
  "dependencies": {
    "@sap-ai-sdk/gen-ai-hub": "workspace:*",
    "@types/express": "^4.17.21",
    "express": "^4.19.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  },
  "scripts": {
    "start": "node --loader ts-node/esm src/index.ts",
    "local": "node --env-file=.env --loader ts-node/esm src/index.ts",
    "build": "tsc",
    "dev": "tsc -w",
    "lint": "eslint . && prettier . --config ../.prettierrc --ignore-path ../.prettierignore -c",
    "lint:fix": "eslint . --fix && prettier . --config ../.prettierrc --ignore-path ../.prettierignore -w --log-level error"
  },
  "engines": {
    "node": "^20"
  }
}
