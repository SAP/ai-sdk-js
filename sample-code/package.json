{
  "type": "module",
  "name": "@sap-ai-sdk/sample-code",
  "version": "0.0.0",
  "private": "true",
  "dependencies": {
    "@sap-ai-sdk/gen-ai-hub": "canary",
    "@types/express": "^4.17.21",
    "@types/express-serve-static-core": "^4.19.2",
    "express": "^4.19.2",
    "ts-node": "^10.9.2",
    "dotenv": "^16.4.5"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**/*.js",
    "dist/**/*.js.map",
    "dist/**/*.d.ts",
    "dist/**/*.d.ts.map"
  ],
  "scripts": {
    "start": "node --loader ts-node/esm --watch src/server.ts",
    "local": "node --env-file=.env --loader ts-node/esm src/server.ts",
    "compile": "tsc",
    "dev": "tsc -w",
    "lint": "eslint . && prettier . --config ../.prettierrc --ignore-path ../.prettierignore -c",
    "lint:fix": "eslint . --fix && prettier . --config ../.prettierrc --ignore-path ../.prettierignore -w --log-level error"
  },
  "engines": {
    "node": "^20"
  }
}
